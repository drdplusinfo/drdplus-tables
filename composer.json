    "granam/strict-object": "dev-master",
    "granam/float": "dev-master",
    "granam/integer": "dev-master",
    "granam/number": "dev-master",
    "granam/boolean": "dev-master",
    "drd-plus/dice-rolls": "dev-master",
    "drd-plus/codes": "dev-master",
    "drd-plus/properties": "dev-master",
    "drd-plus/calculations": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "mockery/mockery": "1.0.x-dev",
    "granam/exceptions-hierarchy": "~4.0"
